{
  "version": 2,
  "functions": {
    "src/controllers/reviews.controller.ts": {
      "maxDuration": 60
    },
    "src/controllers/topics.controller.ts": {
      "maxDuration": 60
    },
    "src/controllers/openai.ts": {
      "maxDuration": 30
    },
    "src/api/**/*.ts": {
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "dest": "src/index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://critech.ashermorse.org",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}